{
  "name": "WIde alias definition schema",
  "description": "This file defines multiple useable enum and namestorage aliases that are useable in schematic files.",
  "version": 1.0,
  "alias": [
    "UnitClass": {
      "type": "ENUM",
      "target": "com.github.naios.wide.entities.enums.UnitClass",
    },
    "UnitFlags": {
      "type": "ENUM",
      "target": "com.github.naios.wide.entities.enums.UnitFlags",
    },
    "Classes": {
      "type": "ENUM",
      "target": "com.github.naios.wide.entities.enums.Classes",
    },
    "Expansion": {
      "type": "ENUM",
      "target": "com.github.naios.wide.entities.enums.Expansion",
    },
    "Races": {
      "type": "ENUM",
      "target": "com.github.naios.wide.entities.enums.Races",
    },
    "CreatureName": {
      "type": "SERVER",
      "database": world
      "target": "creature_template",
      "entryColumn": "entry",
      "nameColumn": "name",
      "prefix": "NPC",
      "zeroName": "none",
      "failPrefix": "unk %s"
    },
    "GameObjectName": {
      "type": "SERVER",
      "database": world
      "target": "gameobject_template",
      "entryColumn": "entry",
      "nameColumn": "name",
      "prefix": "GOB",
      "zeroName": "none",
      "failPrefix": "unk %s"
    },
    "MapName": {
      "type": "CLIENT",
      "target": "Map.dbc",
      "entryColumnIndex": 0,
      "nameColumnIndex": 1,
      "prefix": "MAP",
      "zeroName": "none",
      "failPrefix": "unk %s"
    },
    "SpellName": {
      "type": "CLIENT",
      "target": "Spell.dbc",
      "entryColumnIndex": 0,
      "nameColumnIndex": 1,
      "prefix": "SPELL",
      "zeroName": "none",
      "failPrefix": "unk %s"
    }
  ]
}
